name: picture
<div class="picture">
<object tal:omit-tag="not:self/renderer/vectorimager/enabled" type="image/svg+xml" tal:attributes="data self/vectorimage/url; style string:width:${self/vectorimage/width/em};; height:${self/vectorimage/height/em}"><img tal:attributes="src self/image/url; alt self/source; style string:width:${self/image/width/px};; height:${self/image/height/px}" /></object>
</div>
